{
  "name": "luxuryhomes-backend",
  "version": "1.0.1",
  "description": "Backend for Luxury Homes Rentals",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build": "sequelize db:migrate && sequelize db:seed:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "reseed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sqlite3": "^5.1.7"
  },
  "keywords": ["luxury", "homes", "rentals", "backend"],
  "author": "Shak Gillani",
  "license": "ISC"
}